(declare-const a Bool)
(declare-const b Bool)
(declare-const c Bool)

(push)
(assert (or (or a b) c))
(assert (not a))
(assert (not b))
(assert (not c))
(check-sat)
(pop)

(push)
(assert (not (or a (or b c))))
(assert a)
(check-sat)
(pop)

(push)
(assert (and a (and b c)))
(assert (not a))
(check-sat)
(pop)

(push)
(assert (not (and a (and b c))))
(assert a)
(assert b)
(assert c)
(check-sat)
(pop)