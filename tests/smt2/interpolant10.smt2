(declare-sort I)
(declare-const a I)
(declare-const b I)
(declare-const c I)
(declare-const d I)
(declare-fun p (I) Bool)
(assert (! (= a b) :named ab))
(assert (! (= b c) :named bc))
(assert (! (= c d) :named cd))
(assert (! (p a) :named pa))
(assert (! (not (p d)) :named npd))
(check-sat)
(get-interpolants (and pa ab) (and bc cd npd))
(get-interpolants (and pa npd) (and ab bc cd))
(get-interpolants (and ab bc cd) (and pa npd))
(get-interpolants (and pa npd) (and ab bc cd))
(get-interpolants (and ab bc cd) (and pa npd))
(get-interpolants (and pa bc npd) (and ab cd))
(get-interpolants (and ab cd) (and pa bc npd))